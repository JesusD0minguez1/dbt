<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IdiotTestGame4"
    android:background="@drawable/immers_bg">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/idiotest_4"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/atm"
        android:layout_width="58dp"
        android:layout_height="76dp"
        android:layout_marginStart="108dp"
        android:layout_marginBottom="240dp"
        android:onClick="RightAnswer4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/atm2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/doughnut"
        android:layout_width="123dp"
        android:layout_height="137dp"
        android:layout_marginBottom="255dp"
        android:onClick="WrongAnswer4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/doughnut3"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/pudding"
        android:layout_width="119dp"
        android:layout_height="124dp"
        android:layout_marginBottom="256dp"
        android:onClick="WrongAnswer4"
        android:rotation="-90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/pudding2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/scones"
        android:layout_width="130dp"
        android:layout_height="132dp"
        android:layout_marginTop="271dp"
        android:layout_marginEnd="128dp"
        android:onClick="WrongAnswer4"
        android:rotation="-90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/scones2"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/timer4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="144dp"
        android:text="TextView"
        android:textSize="30dp"
        android:textStyle="bold"
        android:fontFamily="@font/agencyb"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>