<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".IdiotTestGame5"
    android:background="@drawable/immers_bg">

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/idiotest_5"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/jj"
        android:layout_width="77dp"
        android:layout_height="91dp"
        android:layout_marginStart="28dp"
        android:layout_marginBottom="230dp"
        android:onClick="WrongAnswer5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/jjglass2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/kk"
        android:layout_width="82dp"
        android:layout_height="95dp"
        android:layout_marginStart="107dp"
        android:layout_marginBottom="227dp"
        android:onClick="WrongAnswer5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/kkglass2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/ll"
        android:layout_width="80dp"
        android:layout_height="93dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="227dp"
        android:onClick="RightAnswer5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/llglass2"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/mm"
        android:layout_width="76dp"
        android:layout_height="78dp"
        android:layout_marginTop="358dp"
        android:layout_marginEnd="38dp"
        android:onClick="WrongAnswer5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mmglass2"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/timer5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="160dp"
        android:text="TextView"
        android:textSize="40dp"
        android:textStyle="bold"
        android:fontFamily="@font/agencyb"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>